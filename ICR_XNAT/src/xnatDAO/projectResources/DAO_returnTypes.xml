<XNAT_DAO_returnDataTypes>

	<DAO_datatype alias="Imaging session">
		<DAO_subtype alias="MR session">xnat:mrSessionData</DAO_subtype>
		<DAO_subtype alias="PET session">xnat:petSessionData</DAO_subtype>
		<DAO_subtype alias="CT session">xnat:ctSessionData</DAO_subtype>
		<DAO_subtype alias="US session">xnat:usSessionData</DAO_subtype>
		<DAO_subtype alias="Arbitrary session">xnat:imageSessionData</DAO_subtype>
	</DAO_datatype>
	
	<DAO_datatype alias="Set of images">
      <DAO_subtype alias="MR image set">xnat:mrScanData</DAO_subtype>
      <DAO_subtype alias="PET image set">xnat:petScanData</DAO_subtype>
      <DAO_subtype alias="CT image set">xnat:ctScanData</DAO_subtype>
      <DAO_subtype alias="US image set">xnat:usScanData</DAO_subtype>
      <DAO_subtype alias="Arbitrary image set">xnat:imageScanData</DAO_subtype>
   </DAO_datatype>

   <DAO_datatype alias="MR Spectrum">
      <DAO_subtype alias="Any spectrum">xnat:mrsScanData</DAO_subtype>
      <DAO_subtype alias="1-D spectrum">xnat:mrsScanData</DAO_subtype>
      <DAO_subtype alias="2-D spectrum">xnat:mrsScanData</DAO_subtype>
   </DAO_datatype>
   
   <DAO_datatype alias="Parametric images">
      <DAO_subtype alias="Any parametric image">xnat:reconstructedImageData</DAO_subtype>
      <DAO_subtype alias="T1 map">xnat:reconstructedImageData</DAO_subtype>
      <DAO_subtype alias="T2 map">xnat:reconstructedImageData</DAO_subtype>
      <DAO_subtype alias="ADC map">xnat:reconstructedImageData</DAO_subtype>
   </DAO_datatype>

   <DAO_datatype alias="Application outputs">
      <DAO_subtype alias="Any XNAT assessment">xnat:qcAssessmentData</DAO_subtype>
      <DAO_subtype alias="MRIW output">icr:mriwOutputData</DAO_subtype>
      <DAO_subtype alias="Adept output">xnat:qcAssessmentData</DAO_subtype>
   </DAO_datatype>

   <DAO_datatype alias="Regions-of-interest">
      <DAO_subtype alias="Set of ROIs">icr:roiSetData</DAO_subtype>
      <DAO_subtype alias="Single ROI">icr:roiData</DAO_subtype>
   </DAO_datatype>

</XNAT_DAO_returnDataTypes>
