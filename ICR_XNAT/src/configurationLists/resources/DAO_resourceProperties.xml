<XNAT_DAO_resourceProperties>
    
	<DAO_resource code="DicomImage">
      <DAO_resourceProperty name="content">EXTERNAL</DAO_resourceProperty>
		<DAO_resourceProperty name="description">source DICOM image</DAO_resourceProperty>
		<DAO_resourceProperty name="format">DICOM</DAO_resourceProperty>
		<DAO_resourceProperty name="name">DICOM</DAO_resourceProperty>
		<DAO_resourceProperty name="inOut">in</DAO_resourceProperty>
   </DAO_resource>
	
	<DAO_resource code="RoiThumbnail">
      <DAO_resourceProperty name="content">GENERATED</DAO_resourceProperty>
		<DAO_resourceProperty name="description">thumbnail image containing ROI contour</DAO_resourceProperty>
		<DAO_resourceProperty name="format">PNG</DAO_resourceProperty>
		<DAO_resourceProperty name="name">ROI-THUMBNAIL</DAO_resourceProperty>
		<DAO_resourceProperty name="inOut">out</DAO_resourceProperty>
   </DAO_resource>
	
	<DAO_resource code="eRtStruct">
      <DAO_resourceProperty name="content">EXTERNAL</DAO_resourceProperty>
		<DAO_resourceProperty name="description">DICOM-RT structure set file from external application</DAO_resourceProperty>
		<DAO_resourceProperty name="format">PNG</DAO_resourceProperty>
		<DAO_resourceProperty name="name">RT-STRUCT</DAO_resourceProperty>
		<DAO_resourceProperty name="inOut">in</DAO_resourceProperty>
   </DAO_resource>
	
	<DAO_resource code="mRtStruct">
      <DAO_resourceProperty name="content">GENERATED</DAO_resourceProperty>
		<DAO_resourceProperty name="description">derived DICOM-RT structure set file generated from subset of original contours - all ROIs in same file</DAO_resourceProperty>
		<DAO_resourceProperty name="format">DICOM</DAO_resourceProperty>
		<DAO_resourceProperty name="name">RT-STRUCT</DAO_resourceProperty>
		<DAO_resourceProperty name="inOut">out</DAO_resourceProperty>
   </DAO_resource>
	
	<DAO_resource code="sRtStruct">
      <DAO_resourceProperty name="content">GENERATED</DAO_resourceProperty>
		<DAO_resourceProperty name="description">derived DICOM-RT structure set file generated from a single one of original contours</DAO_resourceProperty>
		<DAO_resourceProperty name="format">DICOM</DAO_resourceProperty>
		<DAO_resourceProperty name="name">RT-STRUCT</DAO_resourceProperty>
		<DAO_resourceProperty name="inOut">out</DAO_resourceProperty>
   </DAO_resource>
	
</XNAT_DAO_resourceProperties>
